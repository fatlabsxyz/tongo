{
  "name": "@fatsolutions/she",
  "version": "0.0.1",
  "description": "Homomorphic Encryption over the Stark curve",
  "homepage": "https://github.com/fatlabsxyz/tongo",
  "repository": {
    "type": "git",
    "url": "https://github.com/fatlabsxyz/tongo.git",
    "directory": "packages/she"
  },
  "author": {
      "name": "Fat Solutions",
      "email": "root@fatlabs.xyz",
      "url": "https://fatsolutions.xyz/"
  },
  "keywords": [
      "starknet",
      "homomorphic",
      "elgamal"
  ],
  "files": [
    "lib/*",
    "src",
    "package.json",
    "!**/*.tsbuildinfo"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc -p ./tsconfig.json --outDir lib",
    "test": "vitest run --testTimeout 0 --config vitest.config.ts --passWithNoTests",
    "lint": "eslint --config ../../eslint.config.mjs \"{src,test}/**/*.{js,ts,json}\"",
    "lint:fix": "eslint --config ../../eslint.config.mjs \"{src,test,tests}/**/*.{js,ts,json}\" --fix",
    "format": "prettier --check \"{src,test}/**/*.{js,ts,json}\"",
    "format:fix": "prettier --write \"{src,test,tests}/**/*.{js,ts,json}\""
  },
  "dependencies": {
    "@noble/ciphers": "catalog:",
    "@noble/hashes": "catalog:",
    "@noble/curves": "catalog:",
    "@scure/base": "catalog:",
    "@scure/starknet": "catalog:"
  },
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {},
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
