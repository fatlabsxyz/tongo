{
  "name": "deploy-scripts",
  "version": "1.0.0",
  "description": "Tongo Starknet deployment scripts",
  "type": "module",
  "main": "dist/deploy.js",
  "bin": {
    "deploy": "dist/deploy.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json --outDir dist",
    "deploy": "tsx src/deploy.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "starknet",
    "tongo",
    "deployment"
  ],
  "author": "Fat Solutions",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.4.1",
  "dependencies": {
    "starknet": "8.5.4",
    "chalk": "5.3.0",
    "commander": "12.1.0",
    "inquirer": "12.1.0"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.7",
    "@types/node": "22.13.0",
    "tsx": "4.20.3",
    "typescript": "5.9.2"
  }
}
